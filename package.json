    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-loader": "8.0.2",
    "typedoc": "0.18.0",
    "typescript": "3.9.7",
    "webpack": "4.30.0",
    "webpack-cli": "3.3.2",
    "webpack-dev-server": "3.10.3"
